package week6

object vector {
	//Vector complexity:
	//Adding an element - LOG_32(N)

	val nums = Vector(1,55,3,4)               //> nums  : List[Int] = List(1, 55, 3, 4)
	nums :+ 100                               //> res0: List[Int] = List(1, 55, 3, 4, 100)


	val xs = Array(1,2,3,44)
	
	xs map (x => x*2)

}