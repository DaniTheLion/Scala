package week5

object merge {
 def merge(xs: List[Int], ys: List[Int]): List[Int] =
 (xs,ys) match {
	case(List(), ys) => ys
 	case(xs, List()) => xs
 	case (x :: xsl, y :: ysl) => if (x > y) x :: y :: merge(xsl,ysl) else y :: x :: merge(xsl,ysl)
 }                                                //> merge: (xs: List[Int], ys: List[Int])List[Int]
}