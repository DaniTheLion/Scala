package week5

object high_functions {
  def squareList(xs: List[Int]): List[Int] = xs match {
  	case Nil => Nil
  	case y :: ys => y * y :: ys
  }                                               //> squareList: (xs: List[Int])List[Int]
  
  
  squareList(List(1,2,3,4,5,6))                   //> res0: List[Int] = List(1, 2, 3, 4, 5, 6)
  
  
  
//  def squareList2(xs: List[Int]): List[Int] = xs
  
  
}